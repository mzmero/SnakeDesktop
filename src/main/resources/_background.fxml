      </ImageView>
      <ImageView fx:id="next" fitHeight="75.0" fitWidth="150.0" layoutX="125.0" layoutY="176.0" onMouseClicked="#nextBackground" pickOnBounds="true" preserveRatio="true" smooth="false" GridPane.columnIndex="3" GridPane.halignment="CENTER" GridPane.rowIndex="1">
